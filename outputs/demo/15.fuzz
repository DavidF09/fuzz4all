#include <iostream>
using std::cout; using namespace std;
int main()
{
    cout << "Enter age: ";
    int age;
    cin >> age;
    if( (age < 13) && (age >= 18) ) {
        cout << "Below 13 and over 18" << endl;
    }
    else if( (age < 13) ) {
        cout << "Below 13" << endl;
    }
    else if( (age == 13) ) {
        cout << "Same age" << endl;
    }
    else {
        cout << "Above 18" << endl;
    }
    return 0;
}
